import static org.junit.Assert.*;
import org.junit.Test;
import java.util.ArrayList;
import java.util.List;
 
class check implements StringChecker{
    public boolean checkString(String s){
        return true;
    }
}

public class Tests {
@Test
public void testFilterMethod(){
    List<String> input1 = new Arraylist<>();
    List<String> fliterList = new Arraylist<>();
    input1.add(e: "apple");
    input1.add(e: "banana");
    input1.add(e: "blueberry");
    input1.add(e: "watermelon");
    filterList.add(e: "apple");
    filterList.add(e: "banana");
    filterList.add(e: "blueberry");
    filterList.add(e: "watermelon");
    assertArrayEquals(input1.toArray(), ListExamples.filter(input1, new check()).toArray());
  }
}